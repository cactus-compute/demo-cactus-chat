{
  "expo": {
    "name": "Cactus Chat",
    "slug": "my-app",
    "displayName": "Cactus Chat",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/bw-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rshemetsubuser.myapp",
      "buildNumber": "5",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app does not access your photos or videos.",
        "NSPhotoLibraryAddUsageDescription": "This app does not save photos to your library."
      },
      "appleTeamId": "58U7XV8H43"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bw-logo.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/images/bw-logo.png",
      "package": "com.rshemetsubuser.myapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/bw-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "@react-native-voice/voice"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2c477a6e-5ce8-4386-90ad-7dcee4828ed9"
      }
    },
    "owner": "romansh"
  }
}
